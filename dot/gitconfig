[user]
	name = Manasvi Saxena
	email = msaxena2@illinois.edu
[core]
        #   No editor= means use $VISUAL or $EDITOR.
        excludesfile = ~/.global_gitignore
[merge]
	tool = vimdiff
[alias]
    c = commit
    a = add
    d = diff
    s = show
    st  = status
    l = log
    gr = log --pretty="format:%C(auto)%h%C(auto)%d\\ %Creset%s%C(auto,blue)\\ [%aN]%Creset" --graph
    gra = log --pretty="format:%C(auto)%h%C(auto)%d\\ %Creset%s%C(auto,blue)\\ [%aN]%Creset" --graph --exclude refs/notes/** --exclude refs/stash --all
    d = diff
    r = rebase
    rc = rebase --continue
    ra = rebase --abort
    ri = rebase --interactive
    co = checkout
    cpick = cherry-pick
    stash-st = stash show -p
[color]
	ui = true

[push]
    default = tracking  ; current branch only to tracked upstream branch

[status]
    submoduleSummary = true

[commit]
    verbose = true

[url "git@github.com.:"]
    pushInsteadOf = https://github.com/
